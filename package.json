{
  "name": "@igautomation/playwright-framework",
  "version": "1.0.0",
  "description": "Playwright Test Framework",
  "main": "src/index.js",
  "bin": {
    "playwright-framework": "./bin/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src/",
    "bin/",
    "scripts/",
    "data/",
    "config/",
    "templates/",
    "examples/",
    "docs/",
    ".github/",
    ".husky/",
    "Dockerfile",
    "docker-compose.yml",
    "playwright.config.js",
    "playwright.config.salesforce.js",
    "global-setup.js",
    "jest.config.js",
    "webpack.config.js",
    "tailwind.config.js",
    "postcss.config.js",
    ".eslintrc.js",
    ".prettierrc",
    ".lintstagedrc",
    "install.sh",
    "setup-private-deps.sh"
  ],
  "scripts": {
    "test": "playwright test",
    "test:e2e": "playwright test src/tests/e2e",
    "test:api": "playwright test src/tests/api/salesforce-api-clean.spec.js --config=playwright.config.js",
    "test:api-clean": "playwright test src/tests/api/salesforce-api-clean.spec.js --config=playwright.config.js",
    "test:api-reliable": "playwright test src/tests/api/reliable-api.spec.js --config=playwright.config.js",
    "test:testrail-demo": "playwright test src/tests/api/testrail-demo.spec.js --config=playwright.config.js",
    "test:ui-testrail-demo": "playwright test src/tests/ui/salesforce-ui-testrail-demo.spec.js --config=playwright.config.js",
    "test:ui-testrail-working": "playwright test src/tests/ui/ui-testrail-working.spec.js --config=playwright.config.js",
    "test:ui-testrail-simple": "playwright test src/tests/ui/ui-testrail-simple.spec.js --config=playwright.config.js",
    "test:ui-testrail-final": "playwright test src/tests/ui/ui-testrail-final.spec.js --config=playwright.config.js",
    "test:ci-safe": "playwright test src/tests/api/ci-safe-api.spec.js --config=playwright.config.js",
    "test:ui-ci-safe": "playwright test src/tests/ui/ci-safe-ui.spec.js --config=playwright.config.js",
    "test:api-mock": "playwright test src/tests/api/mock-api-clean.spec.js --config=playwright.config.js",
    "test:accessibility": "playwright test src/tests/accessibility",
    "test:salesforce": "playwright test src/tests/salesforce --config=playwright.config.salesforce.js",
    "test:salesforce:headed": "playwright test src/tests/salesforce --headed",
    "test:salesforce:debug": "playwright test src/tests/salesforce --debug",
    "test:salesforce:ui": "playwright test src/tests/salesforce --ui",
    "test:salesforce:apex": "node scripts/run-apex-tests.js",
    "test:apex-demo": "playwright test src/tests/demo/salesforce-apex-demo.spec.js --config=playwright.config.js",
    "setup:credentials": "node setup-credentials.js",
    "setup:salesforce": "node src/tests/salesforce/global-setup.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "report": "npx playwright show-report",
    "create-bundle": "node scripts/create-bundle.js",
    "docs:dev": "cd docs-site && npm run start",
    "docs:build": "cd docs-site && npm run build",
    "build": "webpack",
    "prepublishOnly": "npm run build",
    "test:salesforce:config": "playwright test --config=playwright.config.salesforce.js",
    "test:salesforce:testrail": "playwright test src/tests/salesforce/salesforce-testrail.spec.js --config=playwright.config.salesforce.js",
    "test:salesforce:api:testrail": "playwright test src/tests/salesforce/salesforce-api-testrail.spec.js --config=playwright.config.salesforce.js",
    "test:salesforce:ui:testrail": "playwright test src/tests/salesforce/salesforce-ui-testrail.spec.js --config=playwright.config.salesforce.js",
    "testrail:demo": "node src/tests/testrail-demo.js",
    "demo:ui": "playwright test src/tests/salesforce-ui-demo.spec.js --config=playwright.config.demo.js",
    "demo:api": "playwright test src/tests/salesforce-api-demo.spec.js --config=playwright.config.demo.js",
    "demo:all": "playwright test src/tests/demo/ --config=playwright.config.demo.js",
    "api:extended": "playwright test src/tests/salesforce-api-extended.spec.js --config=playwright.config.demo.js",
    "api:advanced": "playwright test src/tests/salesforce-api-advanced.spec.js --config=playwright.config.demo.js",
    "api:all": "playwright test src/tests/salesforce-api-*.spec.js --config=playwright.config.demo.js",
    "demo:locators": "playwright test src/tests/salesforce-locators-demo.spec.js --config=playwright.config.demo.js",
    "demo:auth": "playwright test src/tests/salesforce-auth-demo.spec.js --config=playwright.config.demo.js",
    "demo:core": "playwright test src/tests/salesforce-core-demo.spec.js --config=playwright.config.demo.js",
    "demo:testrail": "playwright test src/tests/testrail-core-demo.spec.js --config=playwright.config.demo.js",
    "test:ui": "playwright test src/tests/ui/ --config=playwright.config.js",
    "test:integration": "playwright test src/tests/integration/ --config=playwright.config.js",
    "test:core": "playwright test src/tests/core/ --config=playwright.config.js",
    "test:demo": "playwright test src/tests/demo/ --config=playwright.config.js",
    "test:suite": "playwright test src/tests/integration/suite-testrail.spec.js --config=playwright.config.js",
    "test:clean": "playwright test src/tests/integration/clean-suite.spec.js --config=playwright.config.js",
    "test:testrail": "playwright test src/tests/integration/testrail-mapped-suite.spec.js --config=playwright.config.js",
    "test:debug": "playwright test src/tests/integration/testrail-debug.spec.js --config=playwright.config.js",
    "test:complete": "playwright test src/tests/integration/testrail-complete.spec.js --config=playwright.config.js",
    "test:existing": "playwright test src/tests/integration/testrail-existing-suite.spec.js --config=playwright.config.js",
    "sf:token": "node scripts/generate-sf-token.js",
    "docker:build": "docker build -t playwright-framework .",
    "docker:run": "docker-compose up",
    "docker:test": "docker-compose -f docker-compose.yml up --build",
    "setup:husky": "husky install",
    "setup:project": "node scripts/setup/setup.js",
    "validate:framework": "node scripts/utils/validate-framework.js",
    "health:check": "node scripts/utils/framework-health-check.js",
    "generate:docs": "node scripts/docs/consolidate-docs.js",
    "publish:setup": "node scripts/publishing/setup-publish-token.js",
    "ci:setup": "node scripts/ci-cd/setup-branches.sh",
    "ci:verify-secrets": "node scripts/ci-cd/verify-secrets.js",
    "create:sf-project": "node setup-salesforce-project.js"
  },
  "keywords": [
    "e2e",
    "api",
    "playwright",
    "accessibility",
    "testing",
    "framework",
    "automation"
  ],
  "author": "Your Organization",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "@playwright/test": "^1.40.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "chalk": "^4.1.2",
    "chart.js": "^4.4.0",
    "commander": "^11.1.0",
    "csv-parse": "^5.5.2",
    "dotenv": "^16.6.1",
    "dotenv-safe": "^8.2.0",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "js-yaml": "^4.1.0",
    "jsforce": "^3.8.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-fetch": "^2.7.0",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.53.2",
    "pngjs": "^7.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "allure-playwright": "^2.9.2",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-playwright": "^0.18.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.0.3",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
